{
  "private": true,
  "name": "jvm",
  "version": "1.0.0",
  "description": "Personal website made with React, Flask and GraphQL",
  "scripts": {
    "bootstrap": "cd api && yarn && cd ../web && yarn",
    "build:web": "yarn workspace web build",
    "build:api": "yarn workspace api prestart:prod",
    "start:web": "yarn workspace web dev",
    "start:api": "yarn workspace api start",
    "start": "concurrently \"yarn start:api\" \"yarn start:web\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvnm-dev/jvm.git"
  },
  "author": "Jason Van Malder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jvnm-dev/jvm/issues"
  },
  "homepage": "https://github.com/jvnm-dev/jvm#readme",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "concurrently": "^6.3.0"
  }
}
